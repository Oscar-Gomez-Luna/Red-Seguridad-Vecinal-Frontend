{
    "env": {
        "browser": true,
        "es2021": true,
        "amd": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "define": "readonly",
        "_": "readonly"
    },
    "rules": {
        "no-unused-vars": [
            "warn",
            {
                "args": "none"
            }
        ],
        "no-empty": "warn"
    }
}